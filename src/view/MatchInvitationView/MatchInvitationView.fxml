<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="459.0" prefWidth="378.0" stylesheets="@../css/Main.css"
            xmlns="http://javafx.com/javafx/8.0.121" fx:controller="view.MatchInvitationView.MatchInvitationView">
   <children>
       <TextField fx:id="_searchBar" layoutX="220.0" layoutY="14.0" onKeyReleased="#search" promptText="Zoek speler"
                  AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0"/>
       <ListView fx:id="_playerListView" layoutX="14.0" layoutY="48.0" prefHeight="356.0" prefWidth="347.0"
                 AnchorPane.bottomAnchor="55.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="17.0"
                 AnchorPane.topAnchor="48.0"/>
       <Button layoutX="13.0" layoutY="413.0" mnemonicParsing="false" onAction="#requestGame" prefHeight="34.0"
               prefWidth="347.0" text="Daag uit !" AnchorPane.bottomAnchor="12.0" AnchorPane.leftAnchor="13.0"
               AnchorPane.rightAnchor="18.0"/>
       <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" text="Terug"
               AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="14.0"/>
   </children>
</AnchorPane>
