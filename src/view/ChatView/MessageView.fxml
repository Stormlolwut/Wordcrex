<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../css/Main.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ChatView.MessageView">
   <children>
      <VBox fx:id="_messageVBox" minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: purple;">
         <children>
            <Label fx:id="_messageLabel" maxHeight="1.7976931348623157E308" maxWidth="300.0"
                   style="-fx-background-color: transparent;" styleClass="messageLabel" text="Message" wrapText="true"
                   VBox.vgrow="ALWAYS"/>
            <Label fx:id="_momentLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   style="-fx-background-color: transparent;" styleClass="momentLabel" text="Moment"
                   VBox.vgrow="ALWAYS"/>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
